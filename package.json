{
  "name": "@rocicorp/rails",
  "version": "0.5.0-beta.0",
  "description": "CRUD interface generator for Replicache",
  "homepage": "https://github.com/rocicorp/rails",
  "repository": "github:rocicorp/rails",
  "type": "module",
  "main": "out/index.js",
  "scripts": {
    "format": "prettier --write 'src/*.{js,jsx,json,ts,tsx,html,css,md}' '*.{cjs,js,jsx,json,ts,tsx,html,css,md}'",
    "check-format": "prettier --check 'src/*.{js,jsx,json,ts,tsx,html,css,md}' '*.{cjs,js,jsx,json,ts,tsx,html,css,md}'",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src/",
    "build": "tsc --module ESNext --outDir out",
    "prepack": "npm run lint && npm run test && npm run build",
    "test": "web-test-runner \"src/**/*.test.{jsx,js,ts,tsx}\""
  },
  "author": "",
  "license": "Apache-2.0",
  "peerDependencies": {
    "react": ">=16.0 <19.0",
    "react-dom": ">=16.0 <19.0",
    "replicache": ">=10.0 <14.0 || >12.0.0-beta <12.0.0 || >13.0.0-beta <13.0.0"
  },
  "peerDependenciesMeta": {
    "replicache": {
      "optional": true
    }
  },
  "devDependencies": {
    "@esm-bundle/chai": "^4.3.4-fix.0",
    "@rocicorp/eslint-config": "^0.3.0",
    "@rocicorp/logger": "^4.1.0",
    "@rocicorp/prettier-config": "^0.1.1",
    "@snowpack/web-test-runner-plugin": "^0.2.2",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@web/test-runner": "^0.13.18",
    "nanoid": "^3.3.4",
    "replicache": "13.0.0-beta.0",
    "snowpack": "^3.8.8",
    "typescript": "^4.9.5",
    "zod": "^3.21.4"
  },
  "files": [
    "out/index.d.ts",
    "out/index.js",
    "out/index.d.ts.map",
    "out/index.js.map"
  ],
  "eslintConfig": {
    "extends": "@rocicorp/eslint-config"
  },
  "prettier": "@rocicorp/prettier-config"
}
